<%- include("partials/header"); -%>

<h1>Compose</h1>

  <form action="/compose" method="post">
    <div class="form-group">
    <label for="composeText">Title</label>
    <input class="form-control" type="text" name="postTitle" placeholder="enter Title">
    </div>
    <div class="form-group">
      <label >Post</label>
      <textarea class="form-control" name="postBody" placeholder="type your text.." rows="5" cols="50"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>

  </form>

<%- include("partials/footer"); -%>
